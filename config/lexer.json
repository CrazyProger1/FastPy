{
  "lexer_class": "fastpy.lexer.lexers.Lexer",
  "token_class": "fastpy.lexer.tokens.Token",
  "tokens_detection": {
    "literal": {
      "detector": "fastpy.lexer.detectors.LiteralDetector",
      "regexes": [
        "\"[\\s\\S]+\"",
        "'[\\s\\S]+'"
      ]
    }
  },
  "comment_start": "#"
}