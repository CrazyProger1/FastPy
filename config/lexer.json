{
  "lexer_class": "fastpy.lexer.lexers.Lexer",
  "token_class": "fastpy.lexer.tokens.Token",
  "comment_start": "#",
  "token_detection": {
    "operator": {
      "detector": "fastpy.lexer.detectors.OperatorDetector",
      "regexes": [
        ""
      ]
    },
    "literal": {
      "detector": "fastpy.lexer.detectors.LiteralDetector",
      "regexes": [
        "\"[\\s\\S]+\"",
        "'[\\s\\S]+'"
      ]
    },
    "number": {
      "detector": "fastpy.lexer.detectors.UniversalDetector",
      "regexes": [
        "[\\d]+"
      ]
    },
    "identifier": {
      "detector": "fastpy.lexer.detectors.UniversalDetector",
      "regexes": [
        "[a-zA-Z_][a-zA-Z0-9_]*"
      ]
    }
  }
}